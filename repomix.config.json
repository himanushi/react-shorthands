{
  "output": {
    "filePath": "repomix-output.txt",
    "style": "plain",
    "parsableStyle": false,
    "fileSummary": true,
    "directoryStructure": true,
    "removeComments": false,
    "removeEmptyLines": false,
    "compress": false,
    "topFilesLength": 10,
    "showLineNumbers": false,
    "copyToClipboard": true
  },
  "include": [],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      "tmp/",
      "log/",
      "node_modules/",
      "dist/",
      ".vscode/",
      "*.log",
      "vscode.code-workspace",
      "defaultValues.ts",
      "package-lock.json",
      ".gitignore",
      "values.ts",
      "Ui.stories.tsx",
      "Icon.stories.tsx",
      "constants.ts",
      "styleProcessor.test.ts"
    ]
  },
  "security": {
    "enableSecurityCheck": true
  },
  "tokenCount": {
    "encoding": "o200k_base"
  }
}
